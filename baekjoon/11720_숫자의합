import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int N = sc.nextInt();              // ìˆ«ìì˜ ìë¦¿ìˆ˜
        String number = sc.next();         // ê³µë°± ì—†ì´ ë“¤ì–´ì˜¤ëŠ” ìˆ«ì Nìë¦¬ ë¬¸ìì—´
        int sum = 0;

        for (int i = 0; i < N; i++) {
            // ê° ìë¦¬ ë¬¸ìë¥¼ ì •ìˆ˜ë¡œ ë³€í™˜í•˜ì—¬ ë”í•¨
            sum += number.charAt(i) - '0';
        }

        System.out.println(sum);
    }
}
